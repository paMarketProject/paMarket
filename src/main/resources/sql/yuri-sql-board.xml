<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.pamarket00.mapper.YuriBoardMapper">
    <select id="selectBoardList" resultType="com.example.pamarket00.dto.BoardDto">
        <![CDATA[
        SELECT * from board where boardUserId='aaa'
]]>
    </select>

<!--    글 쓰기 페이지-->
    <insert id="insertProductBoard" parameterType="com.example.pamarket00.dto.BoardDto" useGeneratedKeys="true" keyProperty="boardNum">
        <![CDATA[
        insert into board (boardTitle, boardKategory, boardUserId, boardContents, boardDate, boardProPrice) values(#{title},${kategory},'bbb',${contents},now(),${price});
]]>
    </insert>
</mapper>